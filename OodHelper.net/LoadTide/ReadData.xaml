<Window x:Class="OodHelper.LoadTide.ReadData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ReadData" Height="300" Width="300">
    <Grid>
        <DataGrid Name="TideTable" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Date" Binding="{Binding date}"/>
                <DataGridTextColumn Header="Height" Binding="{Binding height}"/>
                <DataGridTextColumn Header="Current" Binding="{Binding current}"/>
                <DataGridTextColumn Header="Icurrent" Binding="{Binding icurrent}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
